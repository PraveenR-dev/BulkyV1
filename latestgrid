<div id="mainGrid"></div>

<script>
    $(document).ready(function () {
        $("#mainGrid").kendoGrid({
            dataSource: {
                data: [
                    {
                        PageName: "Current Page",
                        TotalCount: 6,
                        Users: [
                            {
                                Username: "Praveen",
                                TotalCount: 4,
                                Details: [
                                    {
                                        LastAccessDate: "17/11/2024",
                                        LastAccessTime: "03:15",
                                        URL: "http://currentpage/activity",
                                        Count: 2
                                    },
                                    {
                                        LastAccessDate: "10/11/2024",
                                        LastAccessTime: "02:15",
                                        URL: "http://currentpage/activity",
                                        Count: 2
                                    }
                                ]
                            },
                            {
                                Username: "Shetty",
                                TotalCount: 2,
                                Details: [
                                    {
                                        LastAccessDate: "18/10/2024",
                                        LastAccessTime: "02:15",
                                        URL: "http://currentpage/measurement",
                                        Count: 1
                                    },
                                    {
                                        LastAccessDate: "19/10/2024",
                                        LastAccessTime: "03:15",
                                        URL: "http://currentpage/measurement",
                                        Count: 1
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        PageName: "Oakford",
                        TotalCount: 8,
                        Users: [
                            {
                                Username: "Praveen",
                                TotalCount: 8,
                                Details: [
                                    {
                                        LastAccessDate: "18/11/2024",
                                        LastAccessTime: "03:20",
                                        URL: "http://oakford/report",
                                        Count: 5
                                    },
                                    {
                                        LastAccessDate: "19/11/2024",
                                        LastAccessTime: "04:30",
                                        URL: "http://oakford/report",
                                        Count: 3
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            pageable: false,
            sortable: true,
            columns: [
                { field: "PageName", title: "Page Name", width: "200px" },
                { field: "TotalCount", title: "Count", width: "100px" }
            ],
            detailTemplate: kendo.template($("#page-users-template").html()),
            detailInit: function (e) {
                $("<div/>").appendTo(e.detailCell).kendoGrid({
                    dataSource: {
                        data: e.data.Users
                    },
                    pageable: false,
                    sortable: true,
                    columns: [
                        { field: "Username", title: "Username", width: "200px" },
                        { field: "TotalCount", title: "Count", width: "100px" }
                    ],
                    detailTemplate: kendo.template($("#user-details-template").html()),
                    detailInit: function (e) {
                        $("<div/>").appendTo(e.detailCell).kendoGrid({
                            dataSource: {
                                data: e.data.Details
                            },
                            pageable: false,
                            sortable: true,
                            columns: [
                                { field: "LastAccessDate", title: "Last Access Date", width: "150px" },
                                { field: "LastAccessTime", title: "Last Access Time", width: "150px" },
                                { field: "URL", title: "URL", width: "250px" },
                                { field: "Count", title: "Count", width: "100px" }
                            ]
                        });
                    }
                });
            }
        });
    });
</script>

<script type="text/x-kendo-template" id="page-users-template">
    <div>
        <h5>Users for #: PageName #</h5>
        <div></div>
    </div>
</script>

<script type="text/x-kendo-template" id="user-details-template">
    <div>
        <h5>Details for #: Username #</h5>
        <div></div>
    </div>
</script>
